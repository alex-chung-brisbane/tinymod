{
    "manifest_version": 3,
    "name": "TinyMOD",
    "description": "External modifications to TinyMCE instances in your browser",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "scripts/content.js"
            ],
            "run_at": "document_end",
            "world": "MAIN"
        }
    ]
}